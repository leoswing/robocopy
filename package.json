{
  "name": "robocopy",
  "version": "0.1.5",
  "description": "robocopy for windows fast copy",
  "keywords": [
    "robocopy",
    "windows",
    "fast-copy"
  ],
  "author": "leoswing",
  "homepage": "",
  "license": "ISC",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "typings": "typings/index.d.ts",
  "files": [
    "dist",
    "typings/*.d.ts"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/leoswing/robocopy.git"
  },
  "scripts": {
    "build:rollup": "cross-env NODE_ENV=production npm-run-all clean prod:rollup",
    "build:tsc": "cross-env NODE_ENV=production npm-run-all clean prod",
    "ts:watch": "tsc --watch",
    "prod:rollup": "rollup -c rollup.config.js",
    "prod": "tsc",
    "build": "npm run build:rollup",
    "clean": "rimraf ./dist",
    "prepublishOnly": "npm run build",
    "release": "standard-version",
    "lint": "eslint --ext .js --ext .ts ./src/"
  },
  "devDependencies": {
    "@rollup/plugin-buble": "^0.21.3",
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@types/cli-spinner": "^0.2.1",
    "@types/node": "^16.0.1",
    "@typescript-eslint/eslint-plugin": "^5.2.0",
    "@typescript-eslint/parser": "^5.2.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.1.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "rollup": "^2.52.8",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "standard-version": "^9.3.0",
    "typescript": "^4.3.5"
  }
}
